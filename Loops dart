import 'dart:html';

void main() {
//  var header = querySelector('#header');
 //header.text = "Alisson";

  //For é para quantidades definidas
for (int i = 0 ; i<10 ; i++){
//  print (i);
}
  // While é para quantidades indefinidas
  int j = 0;
  
  while (j<10){
   // print(j);
    j++;
  }
  
  int k = 0;
  
  do{
    print(k);
    k++;
  }
  while (k<100);
}
